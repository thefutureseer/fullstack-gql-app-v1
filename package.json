{
  "name": "pinky-prom",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "app-install": "npm install --prefix client",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev", 
    "start:prod": "node server.js", 
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "start:dev": "concurrently \"npm run server\" \"cd client && npm run start\""
  },
  "engines": {
    "npm": "8.3.0",
    "node": "18"
  },
  "keywords": [],
  "author": "Daniel T",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.19.0",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^15.5.1",
    "mongoose": "^6.2.2",
    "nodemon": "^2.0.15"
  },

  "devDependencies": {},
  "description": "[![License: MIT](https://img.shields.io/badge/License-MIT-yellow.svg)](https://opensource.org/licenses/MIT)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thefutureseer/fullstack-gql-app-v1.git"
  },
  "bugs": {
    "url": "https://github.com/thefutureseer/fullstack-gql-app-v1/issues"
  },
  "homepage": "https://github.com/thefutureseer/fullstack-gql-app-v1#readme"
}
